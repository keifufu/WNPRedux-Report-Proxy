    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec \"ts-node\" src/index.ts"
  },
  "dependencies": {
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.1",
    "nodemon": "^2.0.20",
